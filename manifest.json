{
  "name": "Less Wutbürger",
  "version": "0.1",
  "description": "Remove Kommentarbereichte with too much Wutbürgers!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}